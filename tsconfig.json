{
    "compilerOptions": {
      "outDir": "public",
      "sourceMap": false,
      "target": "ESNext",
      "useDefineForClassFields": false,
      "module": "esnext",
      "moduleResolution": "node",
      "allowJs": false,
      "strict": true,
      "noUnusedLocals": true,
      "experimentalDecorators": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "removeComments": false,
      "rootDir": "src",
    },
    "include": [
      "src"
    ]
  }